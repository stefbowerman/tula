/* GRID WIDTHS FOR MOBILE FIRST */
$container_mb: 335;
$columns_mb: 2;
$gutter_mb: 25;
$column-width_mb: 300;

$half_mb: $column-width_mb;
$full_mb: $container_mb;

$gutter_mb: percentage($gutter_mb / $container_mb);
$half_mb: percentage($half_mb / $container_mb);
$full_mb: percentage($full_mb / $container_mb);


/* GRID WIDTHS FOR DESKTOP */
$container_dt: 1280;
$columns_dt: 2;
$gutter_dt: 80;
$column-width_dt: 800;

//widths
$half_dt: $column-width_dt;
$full_dt: $container_dt;

//percentage widths
$gutter_dt: percentage($gutter_dt / $container_dt);
$half_dt: percentage($half_dt / $container_dt);
$full_dt: percentage($full_dt / $container_dt);

.items {
  overflow: hidden;
}

.item-sizer {
  width: $gutter_mb;
  @include media-query($large-up) {
    width: $gutter_dt;
  }
}

.column-sizer {
  @include media-query($medium-down) {
    width: $half_mb;
  }
  @include media-query($large-up) {
    width: $half_dt;
  }
}

.item {
  opacity: 0;
  padding-top: 60px;

  @include media-query($large-up) {
    padding-top: 120px;
  }

  &.ready {
    opacity: 1;
  }
}

.item--large-gap {
  @include media-query($large-up) {
    padding-top: 240px;
  }
}



//Mobile block sizes
@include media-query($small) {
  .item--mobile--half {
    width: $half_mb;
  }

  .item--mobile--full {
    width: $full_mb;
    padding-bottom: 60px;
  }

  .first-col--mobile {
    padding-top: 0;
  }

  .item--mobile--full+.item--mobile--full {
    padding-top: 0;
  }
}

//Desktop block sizes
@include media-query($medium-up) {
  .item--desktop--half {
    width: $half_dt;
  }

  .item--desktop--full {
    width: $full_dt;
    padding-bottom: 120px;

    &.item--large-gap {
      padding-bottom: 240px;
    }
  }

  .first-col--desktop {
    padding-top: 0;
  }

  .item--desktop--full+.item--desktop--full {
    padding-top: 0;
  }
}

.item--first {
  padding-top: 0;
  margin-top: 0;
}

.item--last {
  padding-top: 40px;
  padding-bottom: 30px;
}

.item--heading {
  padding-bottom: 60px;
}



.item {
  &.fade-in {
    @include opacity(0);
    transition: all 0.4s cubic-bezier(0.46, 0.01, 0.32, 1);
    -webkit-transform: translateY(2rem);
    transform: translateY(2rem);
  }

  &.activated.fade-in.ready {
    @include opacity(1);
    -webkit-transform: translateY(0);
    transform: translateY(0);
  }
}



@mixin responsive-color($breakpoint, $color, $i) {
  .#{$breakpoint}color--#{$color} {
    color: nth($colors, $i);
  }

  .#{$breakpoint}bg--#{$color} {
    background-color: nth($colors, $i);
  }

  .#{$breakpoint}hv--#{$color} {
    &:hover {
      color: nth($colors, $i);
    }
  }

  .#{$breakpoint}hv-bg--#{$color} {
    &:hover {
      background-color: nth($colors, $i);
    }
  }

  .#{$breakpoint}border--#{$color} {
    border: 1px solid nth($colors, $i);
  }

  .#{$breakpoint}hv--border--#{$color} {
    &:hover {
      border: 1px solid nth($colors, $i);
    }
  }

  .active.#{$breakpoint}active-border--#{$color} {
    border: 1px solid nth($colors, $i);
  }


  .#{$breakpoint}border-bottom--#{$color} {
    border-bottom: 1px solid nth($colors, $i);
  }

  .#{$breakpoint}border-top--#{$color} {
    border-top: 1px solid nth($colors, $i);
  }

  .#{$breakpoint}hv--border-bottom--#{$color} {
    &:hover {
      border-bottom: 1px solid nth($colors, $i);
    }
  }

  .#{$breakpoint}hv--span--border-bottom--#{$color}:hover {
    span {
      border-bottom: 1px solid nth($colors, $i);
    }
  }

  .#{$breakpoint}hv--div--border-bottom--#{$color}:hover {
    > div {
      /*       border-bottom: 1px solid nth($colors, $i); */
    }
  }
}
.active.background--secondary-text{background-color: $color-primary-accent !important}

@each $color in $color-strings {
  $i: index($color-strings, $color);
  @include responsive-color('', $color, $i);
}

@each $breakpoint in $breakpoint-has-widths {
  @include media-query($breakpoint) {
    @each $color in $color-strings {
      $i: index($color-strings, $color);
      @include responsive-color('#{$breakpoint}--', $color, $i);
    }
  }
}


